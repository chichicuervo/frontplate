{
    "name": "frontplate",
    "version": "0.0.1",
    "main": "index.js",
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "build:client": "cross-env NODE_ENV=production webpack --mode production --config config/client.webpack.js",
        "build:server": "cross-env NODE_ENV=production webpack --mode production --config config/server.webpack.js",
        "start:dev": "cross-env NODE_ENV=development nodemon --ignore src/App/ --exec babel-node server.js",
        "start:prod": "node src/server.bundle.js",
        "start": "npm run build && npm run start:prod"
    },
    "author": "Jason Belich <jason@belich.com>",
    "license": "UNLICENSED",
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/node": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.3",
        "@mdx-js/loader": "^1.5.7",
        "babel-loader": "^8.0.6",
        "babel-preset-minify": "^0.5.1",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.1",
        "css-loader": "^3.4.2",
        "file-loader": "^5.1.0",
        "html-webpack-plugin": "^3.2.0",
        "nodemon": "^2.0.2",
        "react-hot-loader": "^4.12.19",
        "regenerator-runtime": "^0.13.3",
        "source-map-support": "^0.5.16",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.42.0",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "@babel/polyfill": "^7.8.7",
        "@loadable/component": "^5.12.0",
        "@loadable/webpack-plugin": "^5.12.0",
        "body-parser": "^1.19.0",
        "lodash-es": "^4.17.15",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "typeface-roboto": "0.0.75"
    }
}
