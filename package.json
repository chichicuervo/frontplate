{
    "name": "frontplate",
    "version": "0.0.1",
    "main": "index.js",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --mode production",
        "start:dev": "cross-env NODE_ENV=development nodemon --ignore src/App/ --exec babel-node server.js",
        "start:prod": "cross-env NODE_ENV=production nodemon --exec babel-node src/server.bundle.js",
        "start": "npm run build && npm run start:prod"
    },
    "author": "Jason Belich <jason@belich.com>",
    "license": "UNLICENSED",
    "devDependencies": {
        "@babel/core": "^7.2.0",
        "@babel/node": "^7.2.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^1.0.0",
        "html-webpack-plugin": "^3.2.0",
        "nodemon": "^1.18.8",
        "react-hot-loader": "^4.6.0",
        "source-map-support": "^0.5.9",
        "webpack": "^4.27.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "webpack-hot-middleware": "^2.24.3",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0",
        "body-parser": "^1.18.3",
        "cross-env": "^5.2.0",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "typeface-roboto": "0.0.54"
    }
}
